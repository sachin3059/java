<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

    <bean id="specification" class="car.example.setter.injection.Specification">
        <property name="make" value="audi" />
        <property name="model" value="audi123" />
    </bean>

    <bean id="myNewCar" class="car.example.setter.injection.Car" autowire="byName">
    </bean>

</beans>